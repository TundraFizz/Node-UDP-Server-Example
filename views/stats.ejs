<!DOCTYPE html>
<html>
<head>
  <title>Fizz Club</title>
  <meta property="og:title"       content="Fizz Mains Club">
  <meta property="og:description" content="🐟 🐟 🐟">
  <meta property="og:image"       content="/img/icon.png">
  <link rel="shortcut icon"       href="/img/icon.png">
  <link rel="stylesheet"          href="/css/index.css">
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.0.2/jquery.min.js"></script>
  <script src="/js/jquery-ui.min.js"></script>
  <script src="/js/jquery-animate-shadow-min.js"></script>
  <script src="/js/jsc.js"></script>
  <script src="/js/index.js"></script>
</head>
<body>
  <!--
  <div>
    <div class="button glow" onclick="Fun3()">Get Names</div>
  </div>
   -->

  <div>
    <table>
      <tr>
        <!-- <th>ID</th> -->
        <th>Avatar</th>
        <th>Name</th>
        <th>Nickname</th>
        <th>Joined</th>
        <th>Roles</th>
        <th>Last Active</th>
      </tr>
      <% for(var i in users){ %>
        <tr>
          <td><img src="<%= users[i]['avatar'] %>" style="width: 100px"></td>
          <td><%= users[i]["username"] %>#<%= users[i]["discriminator"] %></td>
          <td><%= users[i]["nickname"]        %></td>
          <td><%= users[i]["joinedTimestamp"] %></td>

          <td style="text-align: center;">
            <% for(var j in users[i]["roles"]){ var role = users[i]["roles"][j]; %>
              <span style="color: <%= role['color'] %>"><%= role['name'] %></span>
            <% } %>
          </td>

          <td><%= users[i]["lastMessageID"]   %></td>
        </tr>
      <% } %>
    </table>
  </div>
</body>
</html>
